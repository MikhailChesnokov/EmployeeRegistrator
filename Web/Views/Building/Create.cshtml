@model CreateBuildingForm

@{ ViewBag.Title = "Новое здание"; }

<h4>Новое здание</h4>

<form method="post" id="building-form">

    <root-errors/>

    <input-group
        name="Address"
        label="Адрес"
        invalid-feedback="Некорректное название адреса"
        placeholder="Введите адрес ..."
        value="@Model.Address"/>

    <button type="submit" class="btn btn-primary lower-button">Сохранить</button>
</form>